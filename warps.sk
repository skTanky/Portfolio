function warpGUI(p: player):
	set {_gui} to dropper inventory named "&3&lWARPS"
	set slot 0 of {_gui} to {head::warps::spawn} named "&e&lSPAWN" with lore "&7Click to teleport!"
	set slot 1 of {_gui} to {head::warps::crates} named "&6&lCRATES" with lore "&7Click to teleport!"
	set slot 2 of {_gui} to {head::warps::pve} named "&e&lPVE" with lore "&7Click to teleport!"
	set slot 3 of {_gui} to {head::warps::pvp} named "&6&lPVP" with lore "&7Click to teleport"
	set slot 4 of {_gui} to {head::warps::mining} named "&e&lMINING" with lore "&7Click to teleport!"
	set slot 5 of {_gui} to {head::warps::fishing} named "&6&lFISHING" with lore "&7Click to teleport!"
	set slot 6,7,8 of {_gui} to {head::warps::nowarp} named "&c&oComing Soon :)"
	open {_gui} to {_p}

command /warpsadmin [<text>] [<text>]:
	permission: op
	permission message: &cYou do not have permission to do this
	trigger:
		if arg-1 is "set":
			if arg-2 is set:
				if {warp::%arg-2%} isn't set:
					set {warp::%arg-2%} to player's location
					send "{@p} &fYou set warp &b%arg-2% &fto your location"
					
		if arg-1 is "delete":
			if arg-2 is set:
				if {warp::%arg-2%} is set:
					delete {warp::%arg-2%}
					send "{@p} &fYou deleted warp &b%arg-2%"
					
		if arg-1 is "list":
			send "{@p} &7List"
			wait 1 tick
			send "%{warp::*}%"
			
on inventory click:
	if name of event-inventory contains "&3&lWARPS":
		cancel event
		if event-slot = 0:
			send "{@p} &fTeleported you to &b&lSPAWN"
			teleport player to {warp::spawn}
			
		if event-slot = 1:
			send "{@p} &fTeleported you to warp &b&lCRATES"
			teleport player to {warp::crates}
			
		if event-slot = 2:
			send "{@p} &fTeleported you to warp &b&lPVE"
			teleport player to {warp::pve}
			
		if event-slot = 3:
			send "{@p} &fTeleported you to warp &b&lPVP"
			teleport player to {warp::pvp}
			
			
		if event-slot = 4:
			send "{@p} &fTeleported you to warp &b&lMINING"
			teleport player to {warp::mining}
			
		if event-slot = 5:
			send "{@p} &fTeleported you to warp &b&lFISHING"
			teleport player to {warp::fishing}
			
		if event-slot = 6,7 or 8:
			send "{@p} &fWarp doesn't exist"
			
		close player's inventory
			
			

on tab complete:
	event-string = "/warpsadmin"
	set tab completions for position 1 to "set", "delete" and "list"


on command "/warp":
	cancel event
	warpGUI(player)
	
on command "/warps":
	cancel event
	warpGUI(player)
	

	
	
	
	
